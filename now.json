{
    "version": 2.0,
    "name": "enmanuelVideo-api",
    "build": {
        "src":"index.js", "use":"@now/node"
     },
    "routes": [
        { "src": "/(.*)", "dest": "/index.js"}
    ],
    "env": {
        "DB_USER": "@enmanuelvideo-db-user",
        "DB_PASSWORD": "@enmanuelvideo-db-password",
        "DB_HOST": "@enmanuelvideo-db-host",
        "DB_NAME": "@enmanuelvideo-db-name"
    }
}